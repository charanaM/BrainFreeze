<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acer.brainfreeze">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/brf_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutPop"
            android:theme="@style/AppTheme.popstyle"
        ></activity>

        <activity android:name=".ExitPop"
            android:theme="@style/AppTheme.popstyle"
            ></activity>

        <activity android:name=".NewGameMenu"></activity>

        <activity android:name=".GameScreen"></activity>

        <activity android:name=".StatScreen"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>

</manifest>